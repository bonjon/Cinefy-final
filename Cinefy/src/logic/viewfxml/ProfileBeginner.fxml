<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0"
	prefWidth="1480.0" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<BorderPane AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<left>
				<AnchorPane maxWidth="-Infinity" minWidth="-Infinity"
					prefWidth="220.0" style="-fx-background-color: #121212;"
					BorderPane.alignment="CENTER">
					<children>
						<Label layoutX="48.0" layoutY="14.0" text="Cinefy"
							textFill="#f5c518" AnchorPane.topAnchor="14.0">
							<font>
								<Font name="DejaVu Sans Mono Bold" size="33.0" />
							</font>
						</Label>
						<Label fx:id="home" layoutX="75.0" layoutY="320.0"
							onMouseClicked="#onHomeClicked" text="Home" textFill="#f5c518"
							AnchorPane.bottomAnchor="500.0">
							<font>
								<Font name="Arial" size="25.0" />
							</font>
						</Label>
						<Label fx:id="ask" layoutX="85.0" layoutY="351.0"
							onMouseClicked="#onAskClicked" text="Ask" textFill="#f5c518"
							AnchorPane.bottomAnchor="340.0">
							<font>
								<Font name="Arial" size="25.0" />
							</font>
						</Label>
						<Label fx:id="playlists" layoutX="62.0" layoutY="608.0"
							onMouseClicked="#onPlaylistsClicked" text="Playlists"
							textFill="#f5c518" AnchorPane.bottomAnchor="200.0">
							<font>
								<Font name="Arial" size="25.0" />
							</font>
						</Label>
						<Label fx:id="profile" layoutX="73.0" layoutY="676.0"
							text="Profile" textFill="#f5c518" underline="true"
							AnchorPane.bottomAnchor="60.0">
							<font>
								<Font name="Arial Bold Italic" size="25.0" />
							</font>
						</Label>
						<Line endX="100.0" layoutX="110.0" layoutY="71.0"
							startX="-100.0" stroke="#f5c518" />
					</children>
				</AnchorPane>
			</left>
			<center>
				<AnchorPane style="-fx-background-color: #1c1c1c;"
					BorderPane.alignment="CENTER">
					<children>
						<ImageView fx:id="profilePic" fitHeight="250.0"
							fitWidth="300.0" layoutX="14.0" layoutY="57.0"
							pickOnBounds="true" AnchorPane.leftAnchor="20.0"
							AnchorPane.topAnchor="60.0">
							<image>
								<Image url="@../../../res/profilePictures/default.png" />
							</image>
						</ImageView>
						<Label layoutX="320.0" layoutY="60.0" text="Username"
							textFill="#f5c518" AnchorPane.leftAnchor="350.0"
							AnchorPane.topAnchor="60.0">
							<font>
								<Font name="Arial Bold" size="17.0" />
							</font>
						</Label>
						<Label fx:id="username" layoutX="350.0" layoutY="80.0"
							text="username..." textFill="#f5c518"
							AnchorPane.leftAnchor="350.0" AnchorPane.topAnchor="100.0">
							<font>
								<Font name="Arial" size="17.0" />
							</font>
						</Label>
						<Label fx:id="bio" alignment="TOP_LEFT" layoutX="20.0"
							layoutY="310.0" maxHeight="-Infinity" minHeight="-Infinity"
							prefHeight="170.0" prefWidth="500.0" text="bio..."
							textFill="#f5c518" wrapText="true" AnchorPane.leftAnchor="20.0"
							AnchorPane.topAnchor="340.0">
							<font>
								<Font size="18.0" />
							</font>
						</Label>
						<Button fx:id="btnLogout" layoutX="499.0" layoutY="630.0"
							mnemonicParsing="false" onAction="#onLogout"
							style="-fx-background-radius: 30 30 30 30; -fx-background-color: #121212;"
							text="Logout" textFill="#f5c518" AnchorPane.rightAnchor="20.0"
							AnchorPane.topAnchor="650.0">
							<font>
								<Font size="18.0" />
							</font>
						</Button>
					</children>
				</AnchorPane>
			</center>
		</BorderPane>
	</children>
</AnchorPane>
