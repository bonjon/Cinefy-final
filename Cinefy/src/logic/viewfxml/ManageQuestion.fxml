<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1480.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<BorderPane AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<left>
				<AnchorPane maxWidth="-Infinity" minWidth="-Infinity"
					prefWidth="220.0" style="-fx-background-color: #121212;"
					BorderPane.alignment="CENTER">
					<children>
						<Label layoutX="48.0" layoutY="14.0" text="Cinefy"
							textFill="#f5c518" AnchorPane.topAnchor="14.0">
							<font>
								<Font name="DejaVu Sans Mono Bold" size="33.0" />
							</font>
						</Label>
						<Label fx:id="home" layoutX="75.0" layoutY="320.0"
							text="Home" textFill="#f5c518" underline="true"
							AnchorPane.bottomAnchor="500.0">
							<font>
								<Font name="Arial Bold Italic" size="25.0" />
							</font>
						</Label>
						<Label fx:id="profile" layoutX="73.0" layoutY="676.0"
							onMouseClicked="#onProfileClicked" text="Profile"
							textFill="#f5c518" AnchorPane.bottomAnchor="340.0">
							<font>
								<Font name="Arial" size="25.0" />
							</font>
						</Label>
						<Line endX="100.0" layoutX="110.0" layoutY="71.0"
							startX="-100.0" stroke="#f5c518" />
					</children>
				</AnchorPane>
			</left>
			<center>
				<AnchorPane style="-fx-background-color: #1c1c1c;"
					BorderPane.alignment="CENTER">
					<children>
						<Label layoutX="7.0" layoutY="6.0" text="Manage Question"
							textFill="#f5c518" AnchorPane.leftAnchor="25.0"
							AnchorPane.topAnchor="25.0">
							<font>
								<Font name="Arial Bold" size="17.0" />
							</font>
						</Label>
						<ScrollPane layoutX="25.0" layoutY="45.0"
							prefHeight="100.0" style="-fx-background-color: #1c1c1c;"
							AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0"
							AnchorPane.topAnchor="105.0">
							<content>
								<AnchorPane minHeight="100.0" minWidth="0.0"
									prefWidth="1180.0" style="-fx-background-color: #1c1c1c;">
									<children>
										<Label fx:id="labelQuestion" ellipsisString=""
											prefWidth="700.0" text="Domanda" textFill="#ffdf87"
											textOverrun="CLIP" wrapText="true"
											AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
											<font>
												<Font name="Arial Italic" size="19.0" />
											</font>
										</Label>
									</children>
								</AnchorPane>
							</content>
						</ScrollPane>
						<Button fx:id="btnAccept" layoutX="16.0" layoutY="389.0"
							mnemonicParsing="false" onAction="#onAccept" prefHeight="50.0"
							prefWidth="100.0" style="-fx-background-color: GREEN;"
							text="Accept" textFill="WHITE" AnchorPane.leftAnchor="350.0" />
						<Button fx:id="btnReject" layoutX="1123.0" layoutY="390.0"
							mnemonicParsing="false" onAction="#onReject" prefHeight="50.0"
							prefWidth="100.0" style="-fx-background-color: RED;"
							text="Reject" textFill="WHITE" AnchorPane.rightAnchor="350.0" />
					</children>
				</AnchorPane>
			</center>
		</BorderPane>
	</children>
</AnchorPane>
