									prefWidth="350.0" showTickLabels="true"
									style="-fx-control-inner-background: #f5c518; -fx-color: #121212;" />
								<Label layoutX="760.0" layoutY="131.0"
									text="Rate Playlist" textFill="#f5c518">
									<font>
										<Font name="Arial Bold" size="17.0" />
									</font>
								</Label>
								<Button fx:id="btnOk" layoutX="1075.0" layoutY="203.0"
									mnemonicParsing="false" onAction="#onOk"
									style="-fx-background-color: #121212; -fx-background-radius: 30 30 30 30;"
									text="Ok" textFill="#f5c518" AnchorPane.topAnchor="203.0">
									<font>
										<Font name="Arial" size="13.0" />
									</font>
								</Button>
								<Label layoutX="482.0" layoutY="60.0"
									text="Date pubblication" textFill="#f5c518">
									<font>
										<Font name="Arial Bold" size="17.0" />
